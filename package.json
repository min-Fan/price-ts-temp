{
  "name": "linkol-generators",
  "version": "1.0.0",
  "description": "Linkol价格图表和卡片生成器",
  "main": "scripts/chart-generator.ts",
  "scripts": {
    "build": "tsc",
    "chart": "ts-node scripts/chart-generator.ts",
    "card": "ts-node scripts/linkol-card-generator.ts",
    "chart:single": "ts-node scripts/chart-generator.ts single",
    "chart:batch": "ts-node scripts/chart-generator.ts batch",
    "card:single": "ts-node scripts/linkol-card-generator.ts single",
    "card:batch": "ts-node scripts/linkol-card-generator.ts batch",
    "dev:chart": "ts-node-dev --respawn scripts/chart-generator.ts",
    "dev:card": "ts-node-dev --respawn scripts/linkol-card-generator.ts",
    "generate:chart": "npm run chart:single",
    "generate:card": "npm run card:single",
    "batch:chart": "npm run chart:batch",
    "batch:card": "npm run card:batch"
  },
  "keywords": [
    "chart",
    "puppeteer",
    "typescript",
    "linkol",
    "kol"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "dotenv": "^17.2.1",
    "node-fetch": "^3.3.2",
    "puppeteer": "^21.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
